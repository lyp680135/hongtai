@page
@model WarrantyManage.Pages.Manage.Site.ManageModel.InfoModelModel
<html>
<head>
    <meta charset="utf-8">
    <title>模型详情</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    @{ await Html.RenderPartialAsync("_ValidationCssPartial");}
    <style>
        .layui-table td {
            text-indent: 0px;
        }
    </style>
</head>
<body style="padding:5px;">
    @*<div class="place">
        <span>位置：</span>
        <ul class="placeul">
            <li>模型管理</li>
            <li>模型详情</li>
        </ul>
    </div>*@
    <table class="layui-table">
        <thead>
            <tr>
                <td>字段英文名</td>
                <td>字段中文名</td>
                <td>字段类型</td>
                <td>字段长度</td>
                <td>是否为空</td>
                <td>展示类型</td>
                <td>字段初始值</td>
            </tr>
        </thead>
        <tbody>
            @{
                if (Model.BaseManageModel.Count > 0)
                {
                    foreach (var item in Model.BaseManageModel)
                    {
                        <tr>
                            <td>@item.FildName</td>
                            <td>@item.FildDescription</td>
                            <td>@typeof(DataLibrary.EnumList.FileType).GetEnumName(item.FildType)</td>
                            <td>@item.FildLength</td>
                            <td>@typeof(DataLibrary.EnumList.FileIsNull).GetEnumName(item.FildIsNull)</td>
                            <td>@typeof(DataLibrary.EnumList.PageShowType).GetEnumName(item.PageShowType)</td>
                            <td>@item.FildValue</td>                           
                        </tr>
                    }

                }
            }
        </tbody>
    </table>
</body>
</html>