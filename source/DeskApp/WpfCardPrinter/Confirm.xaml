<Window x:Class="WpfCardPrinter.Confirm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCardPrinter"
        mc:Ignorable="d"
        Title="输入口令" Height="240" Width="350"
        ResizeMode="NoResize">
    <Grid>
        <Label Content="请输入清空口令" HorizontalAlignment="Center" FontSize="20" Margin="0 20 0 0"/>
        <PasswordBox Name="txtQueryBatcode" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center" Width="285" Margin="0,75,0,95.4" Height="37" VerticalContentAlignment="Center"/>
        <Button Content="确认清空" HorizontalAlignment="Center" Margin="0,156,0,0" VerticalAlignment="Top" Width="285" Height="37" FontSize="20" Click="Submit_Click"/>
    </Grid>
</Window>
